{
  "name": "web-util",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "nuxt dev --host 0.0.0.0 --port 81",
    "dev:analyze": "nuxt build --analyze",
    "dev:profile": "nuxt dev --host 0.0.0.0 --port 80 --profile",
    "build": "nuxt build",
    "build:analyze": "nuxt build --analyze",
    "build:prod": "cross-env NODE_ENV=production nuxt build",
  "build:report": "nuxt build --analyze && node scripts/analyze-bundle.js",
    "generate": "nuxt generate",
    "preview": "nuxt preview --host 0.0.0.0 --port 80",
    "postinstall": "nuxt prepare",
    "generate:sitemap": "nuxt generate",
    "optimize": "pnpm build:prod && pnpm analyze-bundle",
    "analyze-bundle": "node scripts/analyze-bundle.js",
  "clean": "node -e \"const fs=require('fs');const rm=p=>{if(fs.existsSync(p)){fs.rmSync(p,{recursive:true,force:true});}};['.nuxt','dist','.output'].forEach(rm);console.log('cleaned');\"",
    "lint": "eslint . --fix",
    "perf": "node scripts/performance-check.js"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@nuxtjs/sitemap": "^7.4.3",
    "crypto-js": "^4.2.0",
    "nuxt": "^4.0.3",
    "qrcode": "^1.5.4",
    "vuetify": "^3.7.3"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.12.1",
    "cross-env": "^10.0.0",
    "sass": "^1.77.0"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
